AAA;;AAAA;;AAAA;AAAA;AAAA,yBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CACA,4BADA,EAEA,MAAA,CAAA,WAFA,CAFA;;AAAA;AAAA;AAEA,kBAAA,GAFA,SAEA,IAFA;;AAMA,sBAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;;AAEA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,kBAAA,CAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAaA,KAnEA;AAoEA,IAAA,mBApEA,iCAoEA;AACA,WAAA,KAAA,CAAA,cAAA,CAAA,WAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,WAAA,CAAA,SAAA,GAAA,CAAA;AACA,WAAA,WAAA,CAAA,OAAA,GAAA,CAAA;AACA;AAzEA;AA3DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>商品管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>商品分类</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <el-row>\r\n        <el-col>\r\n          <el-button type=\"primary\" @click=\"showAddCateDialog\">添加分类</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 表格 -->\r\n      <tree-table\r\n        class=\"treeTable\"\r\n        :data=\"catelist\"\r\n        :columns=\"columns\"\r\n        :selection-type=\"false\"\r\n        :show-index=\"true\"\r\n        index-text=\"#\"\r\n        border\r\n        :show-row-hover=\"false\"\r\n      >\r\n        <!-- 是否有效模板列 -->\r\n        <template slot=\"isok\" slot-scope=\"scope\">\r\n          <i\r\n            class=\"el-icon-success\"\r\n            v-if=\"scope.row.cat_deleted === false\"\r\n            style=\"color: lightgreen\"\r\n          ></i>\r\n          <i class=\"el-icon-error\" v-else style=\"color: red;\"></i>\r\n        </template>\r\n        <!-- 排序 -->\r\n        <template slot=\"order\" slot-scope=\"scope\">\r\n          <el-tag size=\"mini\" v-if=\"scope.row.cat_level===0\">一级</el-tag>\r\n          <el-tag type=\"success\" size=\"mini\" v-else-if=\"scope.row.cat_level===1\">二级</el-tag>\r\n          <el-tag type=\"warning\" size=\"mini\" v-else>三级</el-tag>\r\n        </template>\r\n\r\n        <!-- 操作 -->\r\n        <template slot=\"opt\">\r\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\">编辑</el-button>\r\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\">删除</el-button>\r\n        </template>\r\n      </tree-table>\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryinfo.pagenum\"\r\n        :page-sizes=\"[3, 5, 10, 15]\"\r\n        :page-size=\"queryinfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      ></el-pagination>\r\n    </el-card>\r\n\r\n    <el-dialog\r\n      title=\"添加分类\"\r\n      :visible.sync=\"addCateDialogVisible\"\r\n      width=\"50%\"\r\n      @close=\"addCateDialogClosed\"\r\n    >\r\n      <el-form\r\n        :model=\"addCateForm\"\r\n        :rules=\"addCateFormRules\"\r\n        ref=\"addCateFormRef\"\r\n        label-width=\"80px\"\r\n      >\r\n        <el-form-item label=\"分类名称\" prop=\"cat_name\">\r\n          <el-input v-model=\"addCateForm.cat_name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"父级分类\">\r\n          <!-- options指定数据源 -->\r\n          <!-- props指定配置对象 -->\r\n          <el-cascader\r\n            :options=\"parentCateList\"\r\n            :props=\"{expandTrigger:'hover',value:'cat_id', label:'cat_name', children:'children'}\"\r\n            v-model=\"selectedKeys\"\r\n            @change=\"parentCateChanged\"\r\n            clearable\r\n            change-on-select\r\n          ></el-cascader>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addCateDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addCate\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 控制添加分类对话框的显示与隐藏\r\n      addCateDialogVisible: false,\r\n      // 将要添加的分类的数据\r\n      addCateForm: {\r\n        // 分类的名称\r\n        cat_name: '',\r\n        // 父级分类的ID\r\n        cat_pid: 0,\r\n        // 分类的等级，默认为一级\r\n        cat_level: 0\r\n      },\r\n      // 添加分类表单的验证规则对象\r\n      addCateFormRules: {\r\n        cat_name: [\r\n          { required: true, message: '请输入分类名称', trigger: 'blur' }\r\n        ]\r\n      },\r\n      // 商品分类的数据列表\r\n      catelist: [],\r\n      total: 0,\r\n      // 父级分类的列表\r\n      parentCateList: [],\r\n      // 选中的父级分类的ID数组\r\n      selectedKeys: [],\r\n      queryinfo: {\r\n        type: 3,\r\n        pagenum: 1,\r\n        pagesize: 5\r\n      },\r\n      columns: [\r\n        {\r\n          label: '分类名称',\r\n          prop: 'cat_name'\r\n        },\r\n        {\r\n          label: '是否有效',\r\n          // 表示将当前列定义为模板列\r\n          type: 'template',\r\n          template: 'isok'\r\n        },\r\n        {\r\n          label: '排序',\r\n          type: 'template',\r\n          template: 'order'\r\n        },\r\n        {\r\n          label: '操作',\r\n          type: 'template',\r\n          template: 'opt'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  created() {\r\n    this.getCateList()\r\n  },\r\n  methods: {\r\n    // 获取商品分类数据\r\n    async getCateList() {\r\n      const { data: res } = await this.$http.get('/api/private/v1/categories', {\r\n        params: this.queryinfo\r\n      })\r\n      if (res.meta.status !== 200) {\r\n        this.$message.error('获取商品分类信息失败')\r\n      }\r\n      // 把分类数据列表保存到data\r\n      this.catelist = res.data.result\r\n      this.total = res.data.total\r\n    },\r\n    // 监听PageSize\r\n    handleSizeChange(newSize) {\r\n      this.queryinfo.pagesize = newSize\r\n      this.getCateList()\r\n    },\r\n    handleCurrentChange(newPage) {\r\n      this.queryinfo.pagenum = newPage\r\n      this.getCateList()\r\n    },\r\n    showAddCateDialog() {\r\n      // 先获取父级分类的列表\r\n      this.getParentCateList()\r\n      this.addCateDialogVisible = true\r\n    },\r\n    // 获取父级分类的数据列表\r\n    async getParentCateList() {\r\n      const { data: res } = await this.$http.get('/api/private/v1/categories', {\r\n        params: { type: 2 }\r\n      })\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取父级分类失败')\r\n      }\r\n      console.log(res.data)\r\n      this.parentCateList = res.data\r\n    },\r\n    // 选择项发生变化触发这个函数\r\n    parentCateChanged() {\r\n      console.log(this.selectedKeys)\r\n      // 如果 selectedKeys 数组中的length 大于0,证明选中的父级分类\r\n      // 反之，就说明没有选中任何父级分类\r\n      if (this.selectedKeys.length > 0) {\r\n        this.addCateForm.cat_pid = this.selectedKeys[\r\n          this.selectedKeys.length - 1\r\n        ]\r\n        this.addCateForm.cat_level = this.selectedKeys.length\r\n      } else {\r\n        this.addCateForm.cat_pid = 0\r\n        this.addCateForm.cat_level = 0\r\n      }\r\n    },\r\n    addCate() {\r\n      this.$refs.addCateFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        const { data: res } = await this.$http.post(\r\n          '/api/private/v1/categories',\r\n          this.addCateForm\r\n        )\r\n        if (res.meta.status !== 201) {\r\n            this.$message.error('添加分类失败!');\r\n        }\r\n\r\n        this.$message.success('添加分类成功');\r\n        t\r\n      })\r\n    },\r\n    addCateDialogClosed() {\r\n      this.$refs.addCateFormRef.resetFields()\r\n      this.selectedKeys = []\r\n      this.addCateForm.cat_level = 0\r\n      this.addCateForm.cat_pid = 0\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.treeTable {\r\n  margin-top: 15px;\r\n}\r\n.el-cascader {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/goods"}]}