AAA,CAAA,GAAA,CACA,2BAAA,MAAA,CAAA,QAAA,CAAA,EADA,EAEA;AACA,oBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,oBAAA,MAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAFA,mBAFA,CAJA;;AAAA;AAAA;AAIA,kBAAA,GAJA,SAIA,IAJA;;AAAA,wBAYA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAZA;AAAA;AAAA;AAAA;;AAAA,oDAaA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,WAAA,CAbA;;AAAA;AAeA;AACA,kBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA,CAhBA,CAiBA;;AACA,kBAAA,MAAA,CAAA,WAAA,GAlBA,CAmBA;;;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAsBA,KA9FA;AA+FA,IAAA,cA/FA,0BA+FA,EA/FA,EA+FA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,QAAA,CACA,oBADA,EAEA,IAFA,EAGA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAHA,EAQA,KARA,CAQA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBARA,CADA;;AAAA;AACA,gBAAA,aADA;;AAAA,sBAUA,aAAA,KAAA,SAVA;AAAA;AAAA;AAAA;;AAAA,kDAWA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAXA;;AAAA;AAAA;AAAA,uBAaA,MAAA,CAAA,KAAA,CAAA,MAAA,CACA,2BAAA,EADA,CAbA;;AAAA;AAAA;AAaA,gBAAA,GAbA,SAaA,IAbA;;AAAA,sBAgBA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAhBA;AAAA;AAAA;AAAA;;AAAA,kDAiBA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CAjBA;;AAAA;AAmBA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KApHA;AAqHA,IAAA,OArHA,mBAqHA,QArHA,EAqHA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,QAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,uBAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,SAGA,IAHA;;AAAA,sBAIA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAJA;AAAA;AAAA;AAAA;;AAAA,kDAKA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CALA;;AAAA;AAOA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,oBAAA,GAAA,IAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KA9HA;AA+HA;AACA,IAAA,YAhIA,0BAgIA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,oBACA,MAAA,CAAA,cADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,WAAA,CAFA;;AAAA;AAAA;AAAA,uBAIA,MAAA,CAAA,KAAA,CAAA,GAAA,iCAAA,MAAA,CAAA,QAAA,CAAA,EAAA,YAAA;AAAA,kBAAA,GAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CAJA;;AAAA;AAAA;AAIA,gBAAA,GAJA,UAIA,IAJA;;AAAA,sBAKA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GALA;AAAA;AAAA;AAAA;;AAAA,kDAMA,OAAA,CAAA,IAAA,CAAA,GAAA,CANA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AACA,gBAAA,MAAA,CAAA,oBAAA,GAAA,KAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA3IA;AA4IA,IAAA,mBA5IA,iCA4IA;AACA,WAAA,cAAA,GAAA,EAAA;AACA,WAAA,EAAA;AACA;AA/IA;AA1FA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator=\"/\">\r\n      <!-- 面包屑导航区域 -->\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>用户列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <!-- 搜索与添加区域 -->\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-input placeholder=\"请输入内容\" v-model=\"queryInfo.query\" clearable @clear=\"getUserList\">\r\n            <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getUserList\"></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\" @click=\"addDialogVisible=true\">添加用户</el-button>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <!-- 用户列表区 -->\r\n      <el-table :data=\"userlist\" stripe border>\r\n        <el-table-column label=\"#\" type=\"index\"></el-table-column>\r\n        <el-table-column label=\"姓名\" prop=\"username\"></el-table-column>\r\n        <el-table-column label=\"邮箱\" prop=\"email\"></el-table-column>\r\n        <el-table-column label=\"电话\" prop=\"mobile\"></el-table-column>\r\n        <el-table-column label=\"角色\" prop=\"role_name\"></el-table-column>\r\n        <el-table-column label=\"状态\" prop=\"mg_state\">\r\n          <template v-slot=\"s\">\r\n            <el-switch v-model=\"s.row.mg_state\" @change=\"userStateChanged(s.row)\"></el-switch>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <!-- 插槽中的按钮 -->\r\n          <template v-slot=\"s\">\r\n            <el-tooltip\r\n              class=\"item\"\r\n              effect=\"dark\"\r\n              content=\"修改角色\"\r\n              placement=\"top\"\r\n              :enterable=\"false\"\r\n            >\r\n              <!-- 修改按钮 -->\r\n              <el-button\r\n                type=\"primary\"\r\n                icon=\"el-icon-edit\"\r\n                size=\"mini\"\r\n                @click=\"showEditDialog(s.row.id)\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n            <el-tooltip\r\n              class=\"item\"\r\n              effect=\"dark\"\r\n              content=\"分配角色\"\r\n              placement=\"top\"\r\n              :enterable=\"false\"\r\n            >\r\n              <!-- 删除按钮 -->\r\n              <el-button\r\n                type=\"danger\"\r\n                icon=\"el-icon-delete\"\r\n                size=\"mini\"\r\n                @click=\"removeUserById(s.row.id)\"\r\n              ></el-button>\r\n            </el-tooltip>\r\n            <el-tooltip\r\n              class=\"item\"\r\n              effect=\"dark\"\r\n              content=\"分配角色\"\r\n              placement=\"top\"\r\n              :enterable=\"false\"\r\n            >\r\n              <!-- 分配角色按钮 -->\r\n              <el-button type=\"warning\" icon=\"el-icon-setting\" size=\"mini\" @click=\"setRole(s.row)\"></el-button>\r\n            </el-tooltip>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页部分 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1, 2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      ></el-pagination>\r\n    </el-card>\r\n    <!-- 添加用户的对话框 -->\r\n    <el-dialog title=\"添加用户\" :visible.sync=\"addDialogVisible\" width=\"50%\" @close=\"addDialogClosed\">\r\n      <el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"70px\">\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n          <el-input v-model=\"addForm.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\" prop=\"password\">\r\n          <el-input type=\"password\" v-model=\"addForm.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"addForm.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机\" prop=\"mobile\">\r\n          <el-input v-model=\"addForm.mobile\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addUser\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 修改用户的对话框 -->\r\n    <el-dialog title=\"修改用户\" :visible.sync=\"editDialogVisible\" width=\"50%\" @close=\"editDialogClosed\">\r\n      <el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"70px\">\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n          <el-input v-model=\"editForm.username\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"editForm.email\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"手机号\" prop=\"mobile\">\r\n          <el-input v-model=\"editForm.mobile\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editUserInfo\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 分配角色的对话框 -->\r\n    <el-dialog title=\"分配角色\" :visible.sync=\"setRoleDialogVisible\" width=\"50%\" @closed=\"setRoleDialogClosed\">\r\n      <div>\r\n        <p>当前的用户:{{userinfo.username}}</p>\r\n        <p>当前的角色:{{userinfo.role_name}}</p>\r\n        <template>\r\n          <el-select v-model=\"selectedRoleId\" placeholder=\"请选择\">\r\n            <el-option\r\n              v-for=\"item in rolesList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.roleName\"\r\n              :value=\"item.id\"\r\n            ></el-option>\r\n          </el-select>\r\n        </template>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"setRoleDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveRoleInfo\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    var checkEmail = (rule, value, cb) => {\r\n      // 验证邮箱的正则表达式\r\n      const regEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(\\.[a-zA-Z0-9_-])+/\r\n      if (regEmail.test(value)) {\r\n        return cb()\r\n      }\r\n      cb(new Error('请输入合法的邮箱'))\r\n    }\r\n    var checkMobile = (rule, value, cb) => {\r\n      // 验证手机号的正则表达式\r\n      const regMobile = /^(0|86|17951)?(13[0-9]|15[0123456789]|17[678]|18[0-9]|14[57])[0-9]{8}$/\r\n      if (regMobile.test(value)) {\r\n        return cb()\r\n      }\r\n      cb(new Error('请输入合法的手机号'))\r\n    }\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        pagesize: 2\r\n      },\r\n      userlist: [],\r\n      total: 0,\r\n      // 控制添加用户对话框的显示与隐藏\r\n      addDialogVisible: false,\r\n      // 控制编辑角色对话框的显示与隐藏\r\n      editDialogVisible: false,\r\n      // 控制分配角色对话框的显示与隐藏\r\n      setRoleDialogVisible: false,\r\n      // 需要被分配角色的用户信息\r\n      userinfo: {},\r\n      // 分配角色时被选中的角色ID\r\n      selectedRoleId: '',\r\n      // 所有用户的数据列表\r\n      rolesList: [],\r\n      addForm: {\r\n        username: '',\r\n        password: '',\r\n        email: '',\r\n        mobile: ''\r\n      },\r\n      addFormRules: {\r\n        username: [\r\n          {\r\n            min: 3,\r\n            max: 10,\r\n            message: '用户名的长度在3-10个字符之间',\r\n            trigger: 'blur'\r\n          },\r\n          { required: true, message: '请输入用户名', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          {\r\n            min: 6,\r\n            max: 15,\r\n            message: '密码的长度在6-15个字符之间',\r\n            trigger: 'blur'\r\n          },\r\n          { required: true, message: '请输入密码', trigger: 'blur' }\r\n        ],\r\n        email: [\r\n          { required: true, message: '请输入邮箱', trigger: 'blur' },\r\n          { validator: checkEmail }\r\n        ],\r\n        mobile: [\r\n          { required: true, message: '请输入手机号', trigger: 'blur' },\r\n          { validator: checkMobile }\r\n        ]\r\n      },\r\n      // 查询到的用户信息对象\r\n      editForm: {},\r\n      editFormRules: {\r\n        email: [\r\n          { required: true, message: '请输入邮箱', trigger: 'blur' },\r\n          { validator: checkEmail }\r\n        ],\r\n        mobile: [\r\n          { required: true, message: '请输入手机号', trigger: 'blur' },\r\n          { validator: checkMobile }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getUserList()\r\n  },\r\n  methods: {\r\n    async getUserList() {\r\n      const { data: res } = await this.$http.get('/api/private/v1/users', {\r\n        params: this.queryInfo\r\n      })\r\n      // console.log(res)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取用户列表失败')\r\n      }\r\n      this.userlist = res.data.users\r\n      this.total = res.data.total\r\n    },\r\n    // 监听 pagesize 改变的事件\r\n    handleSizeChange(newSize) {\r\n      // console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getUserList()\r\n    },\r\n\r\n    handleCurrentChange(newPage) {\r\n      // console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getUserList()\r\n    },\r\n    // 监听switch开关状态的改变\r\n    async userStateChanged(userinfo) {\r\n      // console.log(userinfo)\r\n      const { data: res } = await this.$http.put(\r\n        `/api/private/v1/users/${userinfo.id}/state/${userinfo.mg_state}`\r\n      )\r\n      if (res.meta.status !== 200) {\r\n        userinfo.mg_state = !userinfo.mg_state\r\n        return this.$message.error('更新用户状态失败')\r\n      }\r\n      this.$message.success('更新用户状态成功')\r\n    },\r\n    // 在退出时，重置添加用户表单\r\n    addDialogClosed() {\r\n      this.$refs.addFormRef.resetFields()\r\n    },\r\n    // 点击按钮，添加新用户\r\n    addUser() {\r\n      this.$refs.addFormRef.validate(async valid => {\r\n        if (!valid) return // 如果不合法\r\n        // 发起网络请求添加新用户\r\n        const { data: res } = await this.$http.post(\r\n          '/api/private/v1/users',\r\n          this.addForm\r\n        )\r\n        if (res.meta.status !== 201) {\r\n          return this.$message.error('添加用户失败')\r\n        }\r\n        this.$message.success('添加用户成功')\r\n        // 隐藏添加用户的对话框\r\n        this.addDialogVisible = false\r\n        this.getUserList()\r\n      })\r\n    },\r\n    async showEditDialog(id) {\r\n      this.editDialogVisible = true\r\n      const { data: res } = await this.$http.get('/api/private/v1/users/' + id)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('查询用户信息失败')\r\n      }\r\n      this.editForm = res.data\r\n      // console.log(this.editForm)\r\n    },\r\n    // 监听修改用户对话框的关闭事件\r\n    editDialogClosed() {\r\n      this.$refs.editFormRef.resetFields()\r\n    },\r\n    editUserInfo() {\r\n      this.$refs.editFormRef.validate(async valid => {\r\n        if (!valid) {\r\n          return\r\n        }\r\n        const { data: res } = await this.$http.put(\r\n          '/api/private/v1/users/' + this.editForm.id,\r\n          {\r\n            email: this.editForm.email,\r\n            mobile: this.editForm.mobile\r\n          }\r\n        )\r\n        // console.log(res)\r\n        if (res.meta.status !== 200) {\r\n          return this.$message.error('用户信息修改失败!')\r\n        }\r\n        // 关闭对话框\r\n        this.editDialogVisible = false\r\n        // 更新数据列表\r\n        this.getUserList()\r\n        // 提示修改成功\r\n        this.$message.success('更新用户信息成功')\r\n      })\r\n    },\r\n    async removeUserById(id) {\r\n      const confirmResult = await this.$confirm(\r\n        '此操作将永久删除该用户, 是否继续?',\r\n        '提示',\r\n        {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }\r\n      ).catch(err => err)\r\n      if (confirmResult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.$http.delete(\r\n        '/api/private/v1/users/' + id\r\n      )\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('删除用户失败')\r\n      }\r\n      this.$message.success('删除用户成功')\r\n      this.getUserList()\r\n    },\r\n    async setRole(userinfo) {\r\n      // 把row中拿到的用户数据赋值给本地，供弹出框调用\r\n      this.userinfo = userinfo\r\n      const { data: res } = await this.$http.get('/api/private/v1/roles')\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取角色信息失败')\r\n      }\r\n      this.rolesList = res.data;\r\n      this.setRoleDialogVisible = true;\r\n    },\r\n    // 点击按钮分配角色\r\n    async saveRoleInfo() {\r\n      if (!this.selectedRoleId) {\r\n        return this.$message.error('请选择用户的角色！');\r\n      }\r\n      const { data: res } = await this.$http.put(`/api/private/v1/users/${this.userinfo.id}/role`, { rid: this.selectedRoleId });\r\n      if (res.meta.status !== 200) {\r\n        return console.warn(res)\r\n      }\r\n      this.$message.success('更新角色成功');\r\n      this.getUserList();\r\n      this.setRoleDialogVisible = false;\r\n    },\r\n    setRoleDialogClosed() {\r\n      this.selectedRoleId = '';\r\n      this.us\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/components/user"}]}