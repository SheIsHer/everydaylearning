;AASA,IAAA,eATA,2BASA,IATA,EASA,OATA,EASA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,QAAA,CACA,mBADA,EAEA,IAFA,EAGA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAHA,EAQA,KARA,CAQA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBARA,CADA;;AAAA;AACA,gBAAA,aADA;;AAAA,sBAUA,aAAA,KAAA,SAVA;AAAA;AAAA;AAAA;;AAAA,kDAWA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAXA;;AAAA;AAaA,gBAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AAbA;AAAA,uBAcA,MAAA,CAAA,KAAA,CAAA,MAAA,iCACA,IAAA,CAAA,EADA,qBACA,OADA,EAdA;;AAAA;AAAA;AAcA,gBAAA,GAdA,SAcA,IAdA;;AAAA,sBAiBA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAjBA;AAAA;AAAA;AAAA;;AAAA,kDAkBA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CAlBA;;AAAA;AAoBA,gBAAA,IAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;;AApBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KA9BA;AA+BA,IAAA,kBA/BA,8BA+BA,IA/BA,EA+BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,MAAA,GAAA,IAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,qBAAA,GAAA,IAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,6BAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,SAGA,IAHA;;AAAA,sBAIA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAJA;AAAA;AAAA;AAAA;;AAAA,kDAKA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CALA;;AAAA;AAOA;AACA,gBAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,WAAA,CAAA,IAAA,EAAA,MAAA,CAAA,OAAA,EATA,CAUA;;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA1CA;AA2CA;AACA,IAAA,WA5CA,uBA4CA,IA5CA,EA4CA,GA5CA,EA4CA;AAAA;;AACA,UAAA,CAAA,IAAA,CAAA,QAAA,EAAA;AACA,eAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA,CAAA,CADA,CACA;AACA;;AACA,MAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,OAAA,EAAA,GAAA;AACA,OAFA;AAGA,KAnDA;AAoDA,IAAA,oBApDA,kCAoDA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,KAtDA;AAuDA;AACA,IAAA,WAxDA,yBAwDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,gCAEA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,cAAA,EAFA,sBAGA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,kBAAA,EAHA;AAKA,gBAAA,KALA,GAKA,IAAA,CAAA,IAAA,CAAA,GAAA,CALA;AAAA;AAAA,uBAQA,MAAA,CAAA,KAAA,CAAA,IAAA,iCAAA,MAAA,CAAA,MAAA,cAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA,CARA;;AAAA;AAAA;AAOA,gBAAA,GAPA,SAOA,IAPA;;AAAA,sBAWA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAXA;AAAA;AAAA;AAAA;;AAAA,kDAYA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CAZA;;AAAA;AAcA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA;;AACA,gBAAA,MAAA,CAAA,YAAA;;AACA,gBAAA,MAAA,CAAA,qBAAA,GAAA,KAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AAzEA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator=\"/\">\r\n      <!-- 面包屑导航区域 -->\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>权限管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>角色列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-card>\r\n      <!-- 添加角色区域 -->\r\n      <el-row>\r\n        <el-col>\r\n          <el-button>添加角色</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 角色列表区域 -->\r\n      <el-table :data=\"rolelist\" border stripe>\r\n        <!-- 展开列 -->\r\n        <el-table-column type=\"expand\" label=\" \">\r\n          <template v-slot=\"s\">\r\n            <el-row\r\n              :class=\"['bdbottom', i1===0?'bdtop':'', 'vcenter']\"\r\n              v-for=\"(item1, i1) in s.row.children\"\r\n              :key=\"item1.id\"\r\n            >\r\n              <!-- 渲染一级权限 -->\r\n              <el-col :span=\"5\">\r\n                <el-tag closable @close=\"removeRightById(s.row, item1.id)\">{{item1.authName}}</el-tag>\r\n                <i class=\"el-icon-caret-right\"></i>\r\n              </el-col>\r\n              <!-- 渲染二级和三级权限 -->\r\n              <el-col :span=\"19\">\r\n                <!-- 通过for循环嵌套渲染二级权限 -->\r\n                <el-row\r\n                  :class=\"[i2===0?'':'bdtop', 'vcenter']\"\r\n                  v-for=\"(item2, i2) in item1.children\"\r\n                  :key=\"item2.id\"\r\n                >\r\n                  <el-col :span=\"6\">\r\n                    <el-tag\r\n                      type=\"success\"\r\n                      closable\r\n                      @close=\"removeRightById(s.row, item2.id)\"\r\n                    >{{item2.authName}}</el-tag>\r\n                    <i class=\"el-icon-caret-right\"></i>\r\n                  </el-col>\r\n                  <el-col :span=\"18\">\r\n                    <el-tag\r\n                      v-for=\"item3 in item2.children\"\r\n                      :key=\"item3.id\"\r\n                      closable\r\n                      @close=\"removeRightById(s.row, item3.id)\"\r\n                    >{{item3.authName}}</el-tag>\r\n                  </el-col>\r\n                </el-row>\r\n              </el-col>\r\n            </el-row>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\r\n        <el-table-column label=\"角色名称\" prop=\"roleName\"></el-table-column>\r\n        <el-table-column label=\"角色描述\" prop=\"roleDesc\"></el-table-column>\r\n        <el-table-column label=\"操作\" width=\"300px\">\r\n          <template v-slot=\"s\">\r\n            <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\">编辑</el-button>\r\n            <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\">删除</el-button>\r\n            <el-button\r\n              type=\"warning\"\r\n              icon=\"el-icon-setting\"\r\n              size=\"mini\"\r\n              @click=\"showSetRightDialog(s.row)\"\r\n            >分配权限</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-card>\r\n    <!-- 分配权限 -->\r\n    <el-dialog\r\n      title=\"提示\"\r\n      :visible.sync=\"setRightDialogVisible\"\r\n      width=\"50%\"\r\n      @close=\"setRightDialogClosed\"\r\n    >\r\n      <el-tree\r\n        ref=\"treeRef\"\r\n        show-checkbox\r\n        :data=\"rightslist\"\r\n        :props=\"treeProps\"\r\n        :default-expand-all=\"true\"\r\n        node-key=\"id\"\r\n        :default-checked-keys=\"defKeys\"\r\n      ></el-tree>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"setRightDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"allowRights\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 所有角色的列表数据\r\n      rolelist: [],\r\n      // 所有权限的数据\r\n      rightslist: [],\r\n      // 控制分配权限对话框的隐藏\r\n      setRightDialogVisible: false,\r\n      // 树形控件的属性绑定对象\r\n      treeProps: {\r\n        label: 'authName',\r\n        children: 'children'\r\n      },\r\n      // 默认选中的节点Id值数组\r\n      defKeys: [],\r\n      // 当前即将分配权限的id\r\n      roleId: ''\r\n    }\r\n  },\r\n  created() {\r\n    this.getRolesList()\r\n  },\r\n  methods: {\r\n    async getRolesList() {\r\n      const { data: res } = await this.$http.get('/api/private/v1/roles')\r\n      if (res.meta.status !== 200) {\r\n        return this.$message('权限列表获取失败')\r\n      }\r\n      this.rolelist = res.data\r\n      // console.log(this.rolelist)\r\n    },\r\n    async removeRightById(role, RightId) {\r\n      const confirmResult = await this.$confirm(\r\n        '此操作将永久删除该权限，是否继续？',\r\n        '提示',\r\n        {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning'\r\n        }\r\n      ).catch(err => err)\r\n      if (confirmResult !== 'confirm') {\r\n        return this.$message.info('取消了删除')\r\n      }\r\n      console.log('确认了删除')\r\n      const { data: res } = await this.$http.delete(\r\n        `/api/private/v1/roles/${role.id}/rights/${RightId}`\r\n      )\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('删除权限失败')\r\n      }\r\n      role.children = res.data\r\n    },\r\n    async showSetRightDialog(role) {\r\n      this.roleId = role.id\r\n      this.setRightDialogVisible = true\r\n      const { data: res } = await this.$http.get('/api/private/v1/rights/tree')\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取权限数据失败')\r\n      }\r\n      // 获取到的权限数据保存到data中\r\n      this.rightslist = res.data\r\n      this.getLeafKeys(role, this.defKeys)\r\n      // console.log(this.rightslist)\r\n    },\r\n    // 通过递归的形式获取所有三级权限的id，并保存到数组中\r\n    getLeafKeys(node, arr) {\r\n      if (!node.children) {\r\n        return arr.push(node.id) // 三级节点\r\n      }\r\n      node.children.forEach(element => {\r\n        this.getLeafKeys(element, arr)\r\n      })\r\n    },\r\n    setRightDialogClosed() {\r\n      this.defKeys = []\r\n    },\r\n    // 点击为角色分配权限\r\n    async allowRights() {\r\n      const keys = [\r\n        ...this.$refs.treeRef.getCheckedKeys(),\r\n        ...this.$refs.treeRef.getHalfCheckedKeys()\r\n      ]\r\n      const idStr = keys.join(',')\r\n      const {\r\n        data: res\r\n      } = await this.$http.post(`/api/private/v1/roles/${this.roleId}/rights`, {\r\n        rids: idStr\r\n      })\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('分配权限失败')\r\n      }\r\n      this.$message.success('分配权限成功！');\r\n      this.getRolesList();\r\n      this.setRightDialogVisible = false;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-tag {\r\n  margin: 7px;\r\n}\r\n.bdtop {\r\n  border-top: 1px solid #eee;\r\n}\r\n.bdbottom {\r\n  border-bottom: 1px solid #eee;\r\n}\r\n.vcenter {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Rights"}]}