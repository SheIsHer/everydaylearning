KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAbA;AAcA,IAAA,gBAdA,4BAcA,OAdA,EAcA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,YAAA;AACA,KAjBA;AAkBA,IAAA,mBAlBA,+BAkBA,OAlBA,EAkBA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,YAAA;AACA,KArBA;AAsBA,IAAA,UAtBA,sBAsBA,EAtBA,EAsBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,QAAA,CAAA,mBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,EAIA,KAJA,CAIA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBAJA,CADA;;AAAA;AACA,gBAAA,aADA;;AAAA,sBAMA,aAAA,KAAA,SANA;AAAA;AAAA;AAAA;;AAAA,kDAOA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAPA;;AAAA;AAAA;AAAA,uBASA,MAAA,CAAA,KAAA,CAAA,MAAA,iCAAA,EAAA,EATA;;AAAA;AAAA;AASA,gBAAA,GATA,SASA,IATA;;AAAA,sBAWA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAXA;AAAA;AAAA;AAAA;;AAAA,kDAYA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAZA;;AAAA;AAcA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,YAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KAtCA;AAuCA,IAAA,SAvCA,uBAuCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,YAAA;AACA;AAzCA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>商品管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>商品列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-input placeholder=\"请输入内容\" v-model=\"queryinfo.query\" clearable @clear=\"getGoodsList\">\r\n            <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"handleCurrentChange(1);getGoodsList\"></el-button>\r\n          </el-input>\r\n        </el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\" @click=\"goAddpage\">添加商品</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- table表格区域 -->\r\n      <el-table :data=\"goodslist\" border stripe>\r\n        <el-table-column type=\"index\" label=\"#\"></el-table-column>\r\n        <el-table-column label=\"商品名称\" prop=\"goods_name\"></el-table-column>\r\n        <el-table-column\r\n          label=\"商品价格\"\r\n          width=\"90px\"\r\n          prop=\"goods_price\"\r\n          :formatter=\"(row)=>{return '￥' + row.goods_price}\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          label=\"商品重量\"\r\n          width=\"120px\"\r\n          prop=\"goods_weight\"\r\n          :formatter=\"(row)=>{return row.goods_weight + 'g'}\"\r\n        ></el-table-column>\r\n        <el-table-column label=\"创建时间\" width=\"120px\" prop=\"add_time\">\r\n          <template v-slot=\"scope\">{{scope.row.add_time | dateFormat}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"200px\" v-slot=\"scope\">\r\n          <template>\r\n            <el-button size=\"mini\" type=\"primary\" icon=\"el-icon-edit\">编辑</el-button>\r\n            <el-button size=\"mini\" type=\"danger\" icon=\"el-icon-delete\" @click=\"removeById(scope.row.goods_id)\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryinfo.pagenum\"\r\n        :page-sizes=\"[5, 10, 15, 20]\"\r\n        :page-size=\"queryinfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n        background\r\n      ></el-pagination>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 查询参数对象\r\n      queryinfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        pagesize: 10\r\n      },\r\n      goodslist: [],\r\n      total: 0\r\n    }\r\n  },\r\n  created() {\r\n    this.getGoodsList()\r\n  },\r\n  methods: {\r\n    // 根据分页获取对应的商品列表\r\n    async getGoodsList() {\r\n      const { data: res } = await this.$http.get('/api/private/v1/goods', {\r\n        params: this.queryinfo\r\n      })\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取商品列表失败！')\r\n      }\r\n      this.$message.success('获取商品列表成功')\r\n      console.log(res.data)\r\n      this.goodslist = res.data.goods\r\n      this.total = res.data.total\r\n    },\r\n    handleSizeChange(newSize) {\r\n      this.queryinfo.pagesize = newSize;\r\n      this.getGoodsList();\r\n    },\r\n    handleCurrentChange(newPage) {\r\n      this.queryinfo.pagenum = newPage;\r\n      this.getGoodsList();\r\n    },\r\n    async removeById(id) {\r\n      const ConfirmResult = await this.$confirm('此操作将永久删除该商品，是否继续？', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (ConfirmResult !== 'confirm') {\r\n        return this.$message.info('已取消删除');\r\n      }\r\n      const { data: res } = await this.$http.delete(`/api/private/v1/goods/${id}`)\r\n      // console.log(res.meta.status)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('删除失败');\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getGoodsList()\r\n    },\r\n    goAddpage() {\r\n      this.$router.push('/goods/add');\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/components/goods"}]}