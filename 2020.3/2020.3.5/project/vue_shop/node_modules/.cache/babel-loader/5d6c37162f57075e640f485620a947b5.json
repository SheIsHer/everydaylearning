YADA,0BACA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,KAAA,CAAA,GAAA,CAAA,wBAAA,EAAA;AACA,kBAAA,MAAA,EAAA,KAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,QACA,IADA;;AAIA,oBAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,gBAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAXA;AAYA,IAAA,gBAZA,4BAYA,OAZA,EAYA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,YAAA;AACA,KAfA;AAgBA,IAAA,mBAhBA,+BAgBA,OAhBA,EAgBA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,YAAA;AACA,KAnBA;AAoBA,IAAA,OApBA,qBAoBA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAtBA;AAuBA,IAAA,eAvBA,6BAuBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,eAAA,GAAA,IAAA;AADA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,GAAA,6CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,SAEA,IAFA;;AAAA,sBAGA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAHA;AAAA;AAAA;AAAA;;AAAA,kDAIA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CAJA;;AAAA;AAMA,gBAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,YAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA/BA;AAgCA,IAAA,mBAhCA,iCAgCA;AACA,WAAA,KAAA,CAAA,cAAA,CAAA,WAAA;AACA;AAlCA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <el-row>\r\n        <el-col :span=\"8\">\r\n          <el-input placeholder=\"请输入内容\" class=\"input-with-select\">\r\n            <el-button slot=\"append\" icon=\"el-icon-search\"></el-button>\r\n          </el-input>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 订单列表数据 -->\r\n      <el-table :data=\"orderlist\" border stripe>\r\n        <el-table-column label=type=\"index\"></el-table-column>\r\n        <el-table-column label=\"订单价格\" prop=\"order_price\"></el-table-column>\r\n        <el-table-column label=\"是否付款\" prop=\"pay_status\">\r\n          <template v-slot=\"scope\">\r\n            <el-tag type=\"success\" v-if=\"scope.row.pay_status === 1\">已付款</el-tag>\r\n            <el-tag type=\"danger\" v-else>未付款</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"是否发货\" prop=\"is_send\"></el-table-column>\r\n        <el-table-column label=\"下单时间\" prop=\"create_time\">\r\n          <template v-slot=\"scope\">{{scope.row.create_time | dateFormat}}</template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template v-slot>\r\n            <el-button size=\"mini\" type=\"primary\" icon=\"el-icon-edit\" @click=\"showBox\"></el-button>\r\n            <el-button size=\"mini\" type=\"success\" icon=\"el-icon-location\" @click=\"showProgressBox\"></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页区 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryinfo.pagenum\"\r\n        :page-sizes=\"[5, 10]\"\r\n        :page-size=\"queryinfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      ></el-pagination>\r\n    </el-card>\r\n    <!-- 修改地址的对话框 -->\r\n    <el-dialog title=\"修改地址\" :visible.sync=\"addressVisible\" width=\"50%\" @close=\"addressDialogClosed\">\r\n      <el-form\r\n        :model=\"addressForm\"\r\n        :rules=\"addressFormRules\"\r\n        ref=\"addressFormRef\"\r\n        label-width=\"100px\"\r\n      >\r\n        <el-form-item label=\"省市区县\" prop=\"address1\">\r\n          <el-cascader :options=\"cityData\" v-model=\"addressForm.address1\"></el-cascader>\r\n        </el-form-item>\r\n        <el-form-item label=\"详细地址\" prop=\"address2\">\r\n          <el-input v-model=\"addressForm.address2\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addressVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addressVisible = false\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 查看物流信息的对话框 -->\r\n    <el-dialog title=\"物流进度\" :visible.sync=\"progressVisible\" width=\"50%\">\r\n      <el-timeline>\r\n        <el-timeline-item>aaa</el-timeline-item>\r\n        <el-timeline-item>bbb</el-timeline-item>\r\n        <el-timeline-item>后台接口有问题，哈哈哈</el-timeline-item>\r\n      </el-timeline>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"progressVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"progressVisible = false\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport cityData from './citydata.js'\r\nexport default {\r\n  data() {\r\n    return {\r\n      queryinfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        pagesize: 10\r\n      },\r\n      total: 0,\r\n      // 控制对话框的显示与隐藏\r\n      addressVisible: false,\r\n      orderlist: [],\r\n      // 地址对话框的属性列表\r\n      addressForm: { address1: [], address2: '' },\r\n      cityData,\r\n      progressVisible: false,\r\n      progressInfo: [],\r\n      addressFormRules: {\r\n        address1: [\r\n          { required: true, message: '请选择省市区县', trigger: 'blur' }\r\n        ],\r\n        address2: [\r\n          { required: true, message: '请填写详细地址', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getOrderList()\r\n  },\r\n  methods: {\r\n    async getOrderList() {\r\n      const { data: res } = await this.$http.get('/api/private/v1/orders', {\r\n        params: this.queryinfo\r\n      })\r\n      if (res.meta.status !== 200) {\r\n        this.$message.error('获取订单列表失败')\r\n      }\r\n      console.log(res)\r\n      this.total = res.data.total\r\n      this.orderlist = res.data.goods\r\n    },\r\n    handleSizeChange(newSize) {\r\n      this.queryinfo.pagesize = newSize\r\n      this.getOrderList()\r\n    },\r\n    handleCurrentChange(newPage) {\r\n      this.queryinfo.pagenum = newPage\r\n      this.getOrderList()\r\n    },\r\n    showBox() {\r\n      this.addressVisible = true;\r\n    },\r\n    async showProgressBox() {\r\n      this.progressVisible = true;\r\n      const { data: res } = await this.$http.get(`/api/private/v1/kuaidi/804909574412544580`)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取物流进度失败')\r\n      }\r\n      this.progressInfo = res.data;\r\n      console.log(this.progressInfo);\r\n    },\r\n    addressDialogClosed() {\r\n      this.$refs.addressFormRef.resetFields()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.el-cascader {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/order"}]}